{
    "manifest_version": 2,
    "name": "IsmaelCurrencyConverter",
    "description": "Copy value and convert from CZK to EUR.",
    "version": "1",
    "background": {
        "scripts": ["event.js"],
        "persistent": false
    },
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "permissions": [
        "tabs", 
        "http://*/*", 
        "https://*/*"
    ]
}